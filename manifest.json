{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "gpluscommunitiesremover.js" ],
        "matches":          [   "http://plus.google.com/*",
                                "https://plus.google.com/*"
                            ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "gpluscommunitiesremover",
    "name":         "gpluscommunitiesremover",
    "version":      "1"
}
